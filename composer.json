{
    "name": "kamerk22/amazongiftcode",
    "description": "Laravel Package for Amazon Gift Codes.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kashyap Merai",
            "email": "kashyapk62@gmail.com",
            "homepage": "https://github.com/kamerk22"
        }
    ],
    "homepage": "https://github.com/kamerk22/amazongiftcode",
    "keywords": [
        "Laravel",
        "AmazonGiftCode",
        "Amazon",
        "GiftCard",
        "AGCOD",
        "Incentives API",
        "Amazon Incentives API"
    ],
    "require": {
        "illuminate/support": "~5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0|^10.5|^11.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0|^9.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "kamerk22\\AmazonGiftCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kamerk22\\AmazonGiftCode\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "kamerk22\\AmazonGiftCode\\AmazonGiftCodeServiceProvider"
            ],
            "aliases": {
                "AmazonGiftCode": "kamerk22\\AmazonGiftCode\\Facades\\AmazonGiftCode"
            }
        }
    }
}
